<h1>Add a new product</h1>

<%= render "layouts/errors", :object => @product %>
<%= form_for @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label "Country Code of origin" %>
  <%= f.text_field :coo %>
  <%= f.label :cost %>
  <%= f.number_field :cost %>
  <%= f.submit %>
<% end %>